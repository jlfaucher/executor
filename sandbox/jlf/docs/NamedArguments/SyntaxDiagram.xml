<syntaxdiagram>
    <title>sd_Instruction_USE</title>
    <groupseq>
        <kwd>USE</kwd>
        <kwd importance="optional">STRICT</kwd>
        <kwd importance="optional">AUTO</kwd>
    </groupseq>
    <groupseq>
        <kwd>NAMED</kwd>
        <kwd>ARG</kwd>
    </groupseq>
    <groupchoice>
        <groupseq>
            <groupseq>
                <repsep>,</repsep>
                <var>name</var>
                <groupseq importance="optional">
                    <delim>(</delim>
